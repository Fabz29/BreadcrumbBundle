{
  "name": "fabz29/breadcrumb-bundle",
  "type": "symfony-bundle",
  "description": "Fabz29BreadcrumbBundle is a Symfony Bundle created to facilitate the management of breadcrumbs in your project.",
  "keywords": [
    "Fabz29 breadcrumb"
  ],
  "require": {
    "php": "^5.5.9 || ^7.0",
    "symfony/framework-bundle": "^2.7 || ^3.0",
    "symfony/security-bundle": "^2.7 || ^3.0",
    "symfony/twig-bundle": "^2.7 || ^3.0",
    "twig/twig": "^1.28 || ^2.0",
    "symfony/templating": "^2.7 || ^3.0",
    "paragonie/random_compat": "^1 || ^2"
  },
  "license": "proprietary software",
  "author": [
    {
      "name": "Fabien Zanetti",
      "email": "fabienzanetti57@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "FOS\\UserBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Lwb\\BreadcrumbBundle\\Tests\\": "/Tests/"
    }
  }
}
