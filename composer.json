{
  "name": "fabz29/breadcrumb-bundle",
  "type": "symfony-bundle",
  "description": "Fabz29BreadcrumbBundle is a Symfony Bundle created to facilitate the management of breadcrumbs in your project.",
  "keywords": [
    "Fabz29 breadcrumb"
  ],
  "require": {
    "php": ">=7.1",
    "symfony/framework-bundle": "^3.0|^4.0"
  },
  "require-dev": {
    "twig/twig": "^2.0",
    "phpunit/phpunit": "8.*"
  },
  "suggest": {
    "twig/twig": "to render breadcrumbs with a twig template"
  },
  "license": "MIT",
  "author": [
    {
      "name": "Fabien Zanetti",
      "email": "fabienzanetti57@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Fabz29\\BreadcrumbBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Fabz29\\BreadcrumbBundle\\Tests\\": "/Tests/"
    }
  }
}
